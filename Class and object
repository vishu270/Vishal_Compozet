
void main() {

  Car myCar = Car('Toyota', 'Corolla', 2020);


  myCar.displayInfo();
}
class Car {
  String brand;
  String model;
  int year;

  
  Car(this.brand, this.model, this.year);

  
  void displayInfo() {
    print('Car Details:');
    print('Brand: $brand');
    print('Model: $model');
    print('Year: $year');
  }
}


OUTPUT:
[Running] dart "c:\Users\lalla\OneDrive\Documents\Desktop\vishal\Dart hello.dart"
Car Details:
Brand: Toyota
Model: Corolla
Year: 2020
